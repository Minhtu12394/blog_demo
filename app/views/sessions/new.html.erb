<% provide(:title, "Log in") %>
<h1>Log in</h1>
<div class="container">
  <div class="col-md-4 col-md-offset-4">
    <div class="innter-form">
      <div class = "sa-innate-form" >
         <%= form_for(:session, url: login_path) do |f| %>
            <%= f.label :email %>
            <%= f.text_field :email %>

            <%= f.label :password %>
            <%= f.password_field :password %>

            <%= f.submit "Log in", class: "btn btn-primary" %>
        <% end %>
      </div>
    </div>
  </div>
</div>

